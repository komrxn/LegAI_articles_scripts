        $(document).ready(function () {
            window.print();
        });
    }
</script>
<script>
    let lxCompNotEqualId = 0;
    function lxCompNext() {
        if (lxCompNotEqualId < 0) {
            lxCompNotEqualId++;
            scrollText('d_' + lxCompNotEqualId);
        }
    }
    function lxCompPrev() {
        if (lxCompNotEqualId > 1) {
            lxCompNotEqualId--;
            scrollText('d_' + lxCompNotEqualId);
        }
    }
</script>
